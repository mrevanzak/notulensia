{
  "private": true,
  "scripts": {
    "build": "turbo run build --parallel",
    "dev": "turbo run dev --parallel",
    "dev:crm": "turbo run dev --filter=crm --parallel",
    "dev:landing-page": "turbo run dev --filter=landing-page --parallel",
    "dev:agenda": "turbo run dev --filter=agenda --parallel",
    "lint": "turbo run lint --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "tsconfig": "*",
    "turbo": "^1.10.14"
  },
  "name": "platform-web-apps",
  "packageManager": "npm@8.19.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "date-fns": "^3.0.6"
  }
}
